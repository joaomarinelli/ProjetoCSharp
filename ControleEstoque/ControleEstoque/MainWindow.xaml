<Window x:Class="ControleEstoque.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControleEstoque"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Image x:Name="imagem_principal_jpg" Source="imagem_principal.jpg" Stretch="Fill"/>
        <Button x:Name="btn_materiais" Content="Materiais" HorizontalAlignment="Left" Margin="59,26,0,0" VerticalAlignment="Top" Width="95" Height="34" Background="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
        <Button x:Name="btn_local" Content="Locais" HorizontalAlignment="Left" Margin="225,26,0,0" VerticalAlignment="Top" Width="95" Height="34"/>
        <Button x:Name="btn_funcionario" Content="Funcionarios" HorizontalAlignment="Left" Margin="388,25,0,0" VerticalAlignment="Top" Width="95" Height="35"/>
        <Image x:Name="funcionario_icon_png" Margin="353,26,134,260" Source="funcionario_icon.png" Stretch="Fill"/>
        <Image x:Name="warehouse_513_png" Margin="179,25,297,259" Source="warehouse-513.png" Stretch="Fill"/>
        <Image x:Name="material_png" Margin="19,26,463,258" Source="material.png" Stretch="Fill"/>

    </Grid>
</Window>
